<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane stylesheets="@MainStyle.css" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.SaveController">
   <children>
      <Label layoutX="258.0" layoutY="61.0" text="Save" textFill="#18239a">
         <font>
            <Font name="System Italic" size="42.0" />
         </font>
      </Label>
      <TextField fx:id="fileName" layoutX="314.0" layoutY="188.0" promptText="File Name" />
      <Label layoutX="100.0" layoutY="191.0" text="Please Insert a File Name to save">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="confirm" layoutX="342.0" layoutY="297.0" mnemonicParsing="false" onAction="#confirm" prefHeight="25.0" prefWidth="66.0" text="Confirm">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="cancel" layoutX="201.0" layoutY="297.0" mnemonicParsing="false" onAction="#cancel" prefHeight="25.0" prefWidth="66.0" text="Cancel">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
