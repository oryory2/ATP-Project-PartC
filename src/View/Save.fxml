<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@MainStyle.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.SaveController">
   <children>
      <Label layoutX="254.0" layoutY="47.0" text="Save" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="42.0" />
         </font>
      </Label>
      <TextField fx:id="fileName" layoutX="333.0" layoutY="188.0" prefHeight="25.0" prefWidth="159.0" promptText="File Name" />
      <Label layoutX="69.0" layoutY="188.0" text="Please Insert a File Name to save">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button fx:id="confirm" layoutX="346.0" layoutY="285.0" mnemonicParsing="false" onAction="#confirm" prefHeight="25.0" prefWidth="66.0" text="Confirm">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="cancel" layoutX="202.0" layoutY="285.0" mnemonicParsing="false" onAction="#cancel" prefHeight="25.0" prefWidth="66.0" text="Cancel">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
