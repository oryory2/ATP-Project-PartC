      <Label alignment="BASELINE_CENTER" contentDisplay="CENTER" layoutX="202.0" layoutY="48.0" prefHeight="72.0" prefWidth="169.0" text="Properties" textAlignment="CENTER" textFill="WHITE" textOverrun="CLIP">
         <font>
            <Font name="System Bold Italic" size="31.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="GeneratingBar" layoutX="334.0" layoutY="173.0" prefWidth="150.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <Blend />
         </effect>
      </ChoiceBox>
      <ChoiceBox fx:id="SolvingBar" layoutX="334.0" layoutY="229.0" prefWidth="150.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <Blend />
         </effect>
      </ChoiceBox>
      <Label alignment="BASELINE_CENTER" layoutX="117.0" layoutY="173.0" prefHeight="25.0" prefWidth="169.0" text="Generating Algorithm">
