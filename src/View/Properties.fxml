            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="confirmButton" layoutX="344.0" layoutY="330.0" mnemonicParsing="false" onAction="#confirmButton" prefHeight="32.0" prefWidth="86.0" text="Confirm">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
   </children>
</AnchorPane>
